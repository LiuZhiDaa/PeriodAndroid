<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <Button
        android:id="@+id/bt1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="加速"/>
    <Button
        android:id="@+id/bt2"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="网络加速"/>
    <Button
        android:id="@+id/bt3"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="降温"/>
    <Button
        android:id="@+id/bt4"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="清理"/>
    <Button
        android:id="@+id/bt5"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="通知"/>
    <Button
        android:id="@+id/bt6"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="充电"/>
    <Button
        android:id="@+id/bt7"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="断电"/>
    <Button
        android:id="@+id/bt8"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="未接电话"/>

    <Button
        android:id="@+id/bt9"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="电池优化" />
</LinearLayout>